e.log(this.caselist[0])\n\n\n    },\n    methods: {\n      fillData () {\n        this.datacollection = {\n          labels: this.datelist,\n          datasets: [\n            {\n              label: 'Data One',\n              backgroundColor: '#f87979',\n              data: this.caselist\n            }\n          ]\n        }\n        \n      }\n   \n    },\n    beforeMount(){\n       console.log(\"before mount\");\n        console.log(this.caselist[0])\n        this.fillData()\n    },\n\n  \n \n  }\n  \n</script>\n\n<style>\n  .small {\n    max-width: 600px;\n    margin:  150px auto;\n  }\n</style>\n"]}]}